{
  "name": "ieee754-buffer",
  "version": "0.2.1",
  "description": "A class to encode and decode IEEE 754 floating point numbers.",
  "homepage": "https://github.com/rochars/ieee754-buffer",
  "author": "Rafael da Silva Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "module": "./ieee754-buffer.js",
  "types": "./ieee754-buffer.d.ts",
  "main": "./ieee754-buffer.umd.js",
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "byte",
    "buffer",
    "binary",
    "parser",
    "number",
    "float",
    "minifloat",
    "ieee754",
    "binary16",
    "binary32",
    "binary64",
    "16-bit",
    "32-bit",
    "64-bit",
    "pack",
    "unpack",
    "encode",
    "decode",
    "serialize",
    "deserialize"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/ieee754-buffer.git"
  },
  "bugs": {
    "url": "https://github.com/rochars/ieee754-buffer/issues"
  },
  "directories": {},
  "files": [
    "ieee754-buffer.js",
    "ieee754-buffer.d.ts",
    "ieee754-buffer.umd.js",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "lint": "jshint ieee754-buffer.js test/src",
    "test": "nyc ./node_modules/mocha/bin/_mocha test/src --recursive -R dot",
    "test-umd": "node ./node_modules/mocha/bin/_mocha test/src --umd --recursive -R dot",
    "test-tsc": "tsc ./test/dist/TypeScript/main.ts && node -r esm ./test/dist/TypeScript/main.js",
    "test-dist": "npm run test-umd && npm run test-tsc",
    "pack": "npm run test && rollup -c && npm run test-dist",
    "doc": "./node_modules/.bin/jsdoc ieee754-buffer.js -d docs -r README.md -t node_modules/docdash",
    "build": "npm run lint && npm run pack && npm run doc",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.2",
    "docdash": "^0.4.0",
    "esm": "^3.0.51",
    "jsdoc": "^3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^12.0.2",
    "rollup": "^0.61.2",
    "rollup-plugin-babel": "^3.0.7",
    "typescript": "^2.9.2"
  },
  "dependencies": {}
}
